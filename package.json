{
  "name": "nds-sandbox",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://nulogy.github.io/nds-sandbox",
  "dependencies": {
    "@nulogy/components": "^5.0.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.3.0",
    "recharts": "^2.0.7",
    "styled-components": "^5.0.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "predeploy": "yarn build",
    "deploy": "gh-pages -d build",
    "prettier-watch": "onchange '**/**/*.js' -- prettier --write {{changed}}",
    "generate:component": "plop component",
    "generate:page": "plop page"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "devDependencies": {
    "eslint": "^6.8.0",
    "eslint-plugin-react": "^7.18.0",
    "gh-pages": "^3.1.0",
    "husky": "^4.2.1",
    "onchange": "^6.1.0",
    "plop": "^2.5.3",
    "prettier": "1.19.1",
    "pretty-quick": "^2.0.1"
  }
}
